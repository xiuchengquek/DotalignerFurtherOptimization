

import os


dot_aligner_files = os.listdir('src/dotaligner_shell_scripts')
dot_aligner_files = [os.path.join('out/',  x) for x in dot_aligner_files ]
dot_aligner_files = [ '%s.dotaligner.out' % x for x in dot_aligner_files ]



rule all:
    input : dot_aligner_files


rule run_script:
    input: 'src/dotaligner_shell_scripts/{files}'
    output:  'out/{files}.dotaligner.out'
    shell : './{input}'









